# Application
APP_ENV=development
APP_LOG_LEVEL=INFO
APP_USER_ID=1

# Planner / APIs
PLANNER_LLM_MODEL=gpt-4o-mini
PLANNER_ALLOW_UNLISTED_SOURCES=false
ALPHA_VANTAGE_API_KEY=
SERPAPI_KEY=
BING_API_KEY=

# LLM Providers
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GROQ_API_KEY=
OLLAMA_ENDPOINT=

# Supabase
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# Browser-Use
BROWSER_USE_ENDPOINT=
BROWSER_USE_TIMEOUT_SECONDS=45

# Twitter Sentiment
TWITTER_QUERY_SINCE=2025-09-01
TWITTER_QUERY_UNTIL=2025-10-15
TWITTER_MIN_REPLIES=100
TWITTER_MIN_LIKES=2000
TWITTER_MIN_RETWEETS=100
TWITTER_USERNAME=
TWITTER_PASSWORD=

# n8n Integration (replaces browser-use for Twitter via HTTP Request)
# Note: No API key needed for webhooks!
N8N_BASE_URL=https://bravecareer.app.n8n.cloud
N8N_TIMEOUT_SECONDS=300

# Email Notifications
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SENDER_EMAIL=your-email@gmail.com
RECIPIENT_EMAILS=recipient1@example.com,recipient2@example.com

